<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="shch_document_datasheet" resource="default" engine="innodb" comment="shch_document_datasheet Table">
		<column xsi:type="int" name="datasheet_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="datasheet_id"/>
		</constraint>
		<column name="file_name" nullable="true" xsi:type="text" comment="file_name"/>
		<column name="title" nullable="false" xsi:type="text" comment="title"/>
	</table>
	<table name="shch_document_drawing" resource="default" engine="innodb" comment="shch_document_drawing Table">
		<column xsi:type="int" name="drawing_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="drawing_id"/>
		</constraint>
		<column name="file_name" nullable="true" xsi:type="text" comment="file_name"/>
		<column name="title" nullable="false" xsi:type="text" comment="title"/>
	</table>
</schema>
